
<h1>Home Page</h1>

<% @hideouts.each do |hideout| %>
<div class="container">
  <div class="row">
    <div class="column">
      <img src="<%= hideout.cover_picture %>" alt="Cover Picture">
      <h2><%= hideout.title %></h2>
        <p><%= hideout.overview %></p>
        <ul>
          <li><%= hideout.price %></li>
        </ul>
    </div>
  </div>
</div>

<% end %>
